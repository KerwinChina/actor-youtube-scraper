{
    "extends": "@apify",
    "env": {
        "mocha": true
    },
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-throw-literal": "off",
        "no-unused-vars": [
            "error",
            {
                "varsIgnorePattern": "should|expect"
            }
        ]
    }
}
